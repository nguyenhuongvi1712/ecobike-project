<SplitPane prefHeight="373.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <AnchorPane prefHeight="317.0" prefWidth="661.0" style="-fx-padding: 10px;">
         <children>
            <ImageView fitHeight="174.0" fitWidth="384.0" layoutX="14.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/dock1.jpeg" />
               </image>
            </ImageView>
            <Label fx:id="dockName" layoutX="14.0" layoutY="199.0" prefHeight="24.0" prefWidth="299.0" text="Dock 3" textFill="#117546" wrapText="true">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
            <Label fx:id="numOfAvailableBikes" layoutX="14.0" layoutY="236.0" prefHeight="16.0" prefWidth="182.0" text="Available bikes : 1" />
            <Label fx:id="maxCapacity" layoutX="14.0" layoutY="264.0" prefHeight="16.0" prefWidth="176.0" text="Empty points : 299" />
            <Line endX="100.0" endY="229.0" layoutX="224.0" layoutY="137.0" startX="100.0" startY="-125.0" />
            <Label fx:id="numOfPoint" layoutX="14.0" layoutY="292.0" prefHeight="16.0" prefWidth="176.0" text="Number of points : 300" />
            <Label fx:id="address" layoutX="347.0" layoutY="157.0" prefHeight="16.0" prefWidth="288.0" text="Address : 300" wrapText="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label fx:id="area" layoutX="14.0" layoutY="318.0" prefHeight="16.0" prefWidth="176.0" text="Area : 300" />
            <ImageView fitHeight="79.0" fitWidth="66.0" layoutX="350.0" layoutY="36.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/map-iconV2.png" />
               </image>
            </ImageView>
            <Label layoutX="435.0" layoutY="36.0" prefHeight="16.0" prefWidth="202.0" text="You are here :" />
            <Label layoutX="435.0" layoutY="70.0" prefHeight="36.0" prefWidth="202.0" text="1 Dai Co Viet Road, Hai Ba Trung Dist., Hanoi" wrapText="true" />
            <Label fx:id="distance" layoutX="347.0" layoutY="191.0" prefHeight="16.0" prefWidth="288.0" text="Distance : 500 m" wrapText="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label fx:id="estimatedTime" layoutX="347.0" layoutY="228.0" prefHeight="16.0" prefWidth="288.0" text="Estimated time : 10 - 11 minutes walk" wrapText="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="backBtn" layoutX="570.0" layoutY="334.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="53.0" style="-fx-background-color: #4dcd77;" text="OK" textFill="WHITE" onMouseClicked="#back"/>
         </children>
      </AnchorPane>
   </items>
</SplitPane>
